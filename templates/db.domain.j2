; {{ ansible_managed }}
;
; BIND data file for zone {{ dhcp_ddns_domain_name }}
;
$TTL	86400
@	IN	SOA	{{ dhcp_ddns_domain_name }}. root.{{ dhcp_ddns_domain_name }}. (
				  1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;
@	IN	NS	ns.{{ dhcp_ddns_domain_name }}.
ns	IN	A	{{ ansible_default_ipv4.address }}
